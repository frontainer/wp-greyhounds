{
    "attr-bans": ["src", "dynsrc", "lowsrc", "onclick", "onload", "ondblclick"],
    "attr-no-dup": true,
    "attr-no-unsafe-char": true,
    "attr-quote-style": "quoted",
    "doctype-first": true,
    "id-class-style": false,
    "indent-style": "spaces",
    "indent-width": 4,
    "tag-bans": ["img", "script", "input", "form", "textarea"],
    "tag-name-lowercase": true,
    "tag-name-match": true
}